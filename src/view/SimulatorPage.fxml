<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="Controller.SimulatorPageController"
            prefHeight="400.0" prefWidth="600.0">

        <HBox AnchorPane.topAnchor="0.0"
              AnchorPane.rightAnchor="0.0"
              AnchorPane.bottomAnchor="0.0"
              AnchorPane.leftAnchor="0.0"
              spacing="20" fillHeight="true">

            <AnchorPane HBox.hgrow="ALWAYS">
                <fx:include source="ScriptEditor.fxml" HBox.hgrow="ALWAYS"/>
            </AnchorPane>

            <AnchorPane HBox.hgrow="ALWAYS">
                <fx:include source="GraphDisplay.fxml" HBox.hgrow="ALWAYS"/>
            </AnchorPane>
        </HBox>
</AnchorPane>
